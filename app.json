{
  "expo": {
    "name": "MindMate-Build",
    "slug": "mindmate-build",
    "owner": "joyounghyun",
    "scheme": "mindmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joyounghyun.MindMate",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.joyounghyun.MindMate"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app uses the photo library to select images.",
          "cameraPermission": "This app uses the camera to take photos."
        }
      ],
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "4184bbd7-58e2-498b-ac87-4bc062c6f6a3"
      }
    }
  }
}
